{
  "name": "Screen Sharing Extension",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "desktopCapture",
    "tabs",
    "activeTab"
  ],
  "action": {},
  "web_accessible_resources": [{
    "resources": ["index.html", "app.js"],
    "matches": ["<all_urls>"]
  }]
}
